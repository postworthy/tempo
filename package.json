{
  "name": "tempo-template",
  "version": "0.1.0",
  "private": true,
  "description": "Clone-and-start template for human + AI collaboration",
  "license": "MIT",
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "bootstrap:check": "node scripts/bootstrap-check.mjs",
    "check:docs": "node scripts/check-docs.mjs",
    "test": "vitest run",
    "build": "tsc -p tsconfig.json",
    "verify:fast": "pnpm format:check && pnpm lint && pnpm typecheck",
    "verify": "pnpm verify:fast && pnpm check:docs && pnpm test && pnpm build"
  },
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@types/node": "^22.13.1",
    "eslint": "^9.20.1",
    "globals": "^15.15.0",
    "prettier": "^3.5.3",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.1",
    "vitest": "^3.0.5"
  }
}
